{
  "manifest_version": 2,
  "name": "Simple PONG",
  "description": "Simple and lightweight PONG Game written in JavaScript",
  "version": "1.5",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "game.html"
  },
  "permissions": [
    "activeTab"
  ]
}
