{
  "manifest_version": 2,

  "name": "Simple PONG",
  "description": "Simple and lightweight PONG Game written in JavaScript",
  "version": "1.1",
  "icons": { "128": "pong.png" },

  "browser_action": {
    "default_icon": "pong.png",
    "default_popup": "game.html"
  },
  "permissions": [
    "activeTab"
  ]
}
